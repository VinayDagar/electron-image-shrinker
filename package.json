{
  "name": "imageshrinker",
  "productname": "Image Shrink",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --prune=false --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Image Shrink\"",
    "package-linux": "electron-packager . --overwrite  --platform=linux --arch=x64 --prune=fase --out=release-builds"
  },
  "author": "VinayDagar",
  "license": "ISC",
  "devDependencies": {
    "electron": "^11.1.1",
    "electron-packager": "^15.2.0"
  },
  "dependencies": {
    "electron-log": "^4.3.1",
    "imagemin": "^7.0.1",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.1",
    "nodemon": "^2.0.7",
    "slash": "^3.0.0"
  }
}
